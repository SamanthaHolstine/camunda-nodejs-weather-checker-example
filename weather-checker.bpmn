  <bpmn:process id="node-slackbot" name="Weather Checker" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Start process with specific city">
      <bpmn:extensionElements />
      <bpmn:outgoing>Flow_0z7iuf8</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:serviceTask id="Activity_1p1d6md" name="Check the weather" zeebe:modelerTemplate="io.camunda.connectors.HttpJson.v2" zeebe:modelerTemplateVersion="7" zeebe:modelerTemplateIcon="data:image/svg+xml;base64,">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="io.camunda:http-json:1" retries="3" />
        <zeebe:ioMapping>
          <zeebe:input source="noAuth" target="authentication.type" />
          <zeebe:input source="GET" target="method" />
          <zeebe:input source="=&#34;https://api.weatherbit.io/v2.0/current?city=&#34; + city + &#34;&#38;key={{secrets.WeatherBit}}&#34;" target="url" />
          <zeebe:input source="20" target="connectionTimeoutInSeconds" />
          <zeebe:input source="20" target="readTimeoutInSeconds" />
        </zeebe:ioMapping>
        <zeebe:taskHeaders>
          <zeebe:header key="resultVariable" value="result" />
          <zeebe:header key="resultExpression" value="={&#10;temperature: response.body.data.temp,&#10;  feels_like: response.body.data.app_temp&#10;}" />
          <zeebe:header key="retryBackoff" value="PT0S" />
        </zeebe:taskHeaders>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0z7iuf8</bpmn:incoming>
      <bpmn:outgoing>Flow_1q12hk9</bpmn:outgoing>
    </bpmn:serviceTask>
